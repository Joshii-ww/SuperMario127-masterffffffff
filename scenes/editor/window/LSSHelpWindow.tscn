[sub_resource type="GDScript" id=12]
script/source = "extends ScrollContainer

onready var autotiling_container = get_node(\"../Autotiling\")

onready var hotkeys_button = get_node(\"../HotkeysButton\")
onready var autotiling_button = get_node(\"../AutotilingButton\")

func _ready():
	var _connect = hotkeys_button.connect(\"pressed\", self, \"switch_to_hotkeys\")
	_connect = autotiling_button.connect(\"pressed\", self, \"switch_to_autotiling\")

func switch_to_hotkeys():
	autotiling_container.visible = false
	autotiling_button.disabled = false
	
	hotkeys_button.disabled = true
	visible = true

func switch_to_autotiling():
	autotiling_container.visible = true
	autotiling_button.disabled = true
	
	hotkeys_button.disabled = false
	visible = false
"

